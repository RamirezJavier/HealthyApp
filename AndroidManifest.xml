<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.javierramirez.healthyapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar"
        android:name="com.orm.SugarApp">
        <activity android:name=".Inicio">
            <meta-data android:name="DATABASE" android:value="healthy.db" />
            <meta-data android:name="VERSION" android:value="1" />
            <meta-data android:name="QUERY_LOG" android:value="true" />
            <meta-data android:name="DOMAIN_PACKAGE_NAME" android:value="com.javierramirez.healthyapp" />

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UsuarioInterfaz" />
        <activity android:name=".UserSettings" />
        <activity android:name=".Dietas"/>
        <activity android:name=".Ejercicios"/>
        <activity android:name=".Info"/>
        <activity android:name=".InterfazHistorial"/>
    </application>

</manifest>